{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "jquery": true
    },
    "globals": {
      "Bloodhound": true,
      "Promise": true
    },
    "rules": {
      "no-console": 0,
      "react/forbid-prop-types": 0
    }
}
